#include <iostream>
#include <string>
using namespace std;
int main()
{
	char str[20];
	int len;
	cout << "Input String\t:";
	cin >> str ;
	len = strlen(str);
	cout << "Reverse String\t:";
	for (int i=len-1;i >= 0 ; i--)
	{
		cout << (char)toupper(str[i]) ;

	}
	cout << endl;

return 0;
}